<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Github.Fonts">
    </style>

    <style name="Github.Fonts.H2_Headline">
        <item name="android:fontFamily">@font/rubik_light</item>
        <item name="android:textSize">36sp</item>
    </style>

    <style name="Github.Fonts.H5_Headline">
        <item name="android:fontFamily">@font/rubik_medium</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Github.Fonts.H6_Headline">
        <item name="android:fontFamily">@font/rubik_medium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Github.Fonts.Subtitle2" tools:keep="@style/Budget/Fonts/Subtitle2">
        <item name="android:fontFamily">@font/rubik_medium</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="Github.Fonts.Body1">
        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Github.Fonts.BUTTON">
        <item name="android:fontFamily">@font/rubik_medium</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="Github.Fonts.Caption">
        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="android:textSize">8sp</item>
    </style>

</resources>